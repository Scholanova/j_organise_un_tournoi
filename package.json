{
  "name": "myplayoff",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "node ./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "test": "node ./node_modules/mocha/bin/_mocha --recursive",
    "db:init": "node ./node_modules/sequelize-cli/lib/sequelize db:create",
    "db:migrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "db:seed": "node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "db:reset": "node ./node_modules/sequelize-cli/lib/sequelize db:drop && npm run db:init && npm run db:migrate",
    "db:test:init": "set NODE_ENV=test&& node ./node_modules/sequelize-cli/lib/sequelize db:create",
    "db:test:migrate": " set NODE_ENV=test&& node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "db:test:reset": "set NODE_ENV=test&& node ./node_modules/sequelize-cli/lib/sequelize db:drop && npm run db:test:init && npm run db:test:migrate"
      },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0"
  }
}
